

@if (TempData.ContainsKey(GlobalConstants.TempDataSuccessMessageKey))
{
    <div class="alert alert-success text-center">@TempData[GlobalConstants.TempDataSuccessMessageKey]</div>
}
@if (TempData.ContainsKey(GlobalConstants.TempDataErrorMessageKey))
{
    <div class="alert alert-danger text-center">@TempData[GlobalConstants.TempDataErrorMessageKey]</div>
}